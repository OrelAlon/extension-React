{
  "name": "Arpeely Chrome Extension",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Chrome Extension",
    "default_popup": "index.html",
    "default_icon": {
      "16": "diamond.png",
      "128": "diamond.png"
    }
  },
  "permissions": ["activeTab", "tabs", "storage"]
}
